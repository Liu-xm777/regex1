   "source": [
    "import re\n",
    "b= input('請輸入一個email地址')\n",
    "regex = re.compile(r'([A-Za-z0-9]+[.-_])*[A-Za-z0-9]+@[A-Za-z0-9-]+(\\.[A-Z|a-z]{2,})+')\n",
    "\n",
    "def isValid(email):\n",
    "    if re.fullmatch(regex, email):\n",
    "        print('有效的email地址')\n",
    "    else:\n",
    "        print('无效的email地址')\n",
    "\n",
    "isValid(b)"
   ]
